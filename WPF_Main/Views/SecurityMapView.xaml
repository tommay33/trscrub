             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:"
             xmlns:vm="clr-namespace:SecurityMap"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.DataContext>
        <vm:SecurityMapViewModel/>
    </UserControl.DataContext>
    <UserControl.Resources>
        <Style TargetType="Button">
           
        </Style>
        
    </UserControl.Resources>
    <Grid>
        <Calendar HorizontalAlignment="Left" Margin="611,62,0,0" VerticalAlignment="Top"/>
 
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="100" VerticalAlignment="Bottom" >
            <Button  Width="91" Margin="0,70,0,0"  Content="{Binding Button1Text}"/>
            <Button  Width="97" Margin="0,70,0,0" />
        </StackPanel>
       

    </Grid>
</UserControl>
